<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="mypaydbmap.BeaconEntity" table="beacon" schema="mypay">
        <id name="idbeacon">
            <column name="idbeacon" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="uuid">
            <column name="uuid" sql-type="varchar(200)" length="200"/>
        </property>
        <property name="majorid">
            <column name="majorid" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="minorid">
            <column name="minorid" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="idstore">
            <column name="idstore" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>