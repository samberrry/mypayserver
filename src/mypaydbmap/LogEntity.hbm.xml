<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="mypaydbmap.LogEntity" table="log" schema="mypay">
        <id name="idlog">
            <column name="idlog" sql-type="int(11)"/>
        </id>
        <property name="number">
            <column name="number" sql-type="int(11)"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(16)" length="16"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(250)" length="250"/>
        </property>
    </class>
</hibernate-mapping>